@model Event
@{ ViewData["Title"] = "Detail"; }

<div class="row">
    <img src="~/assets/img/event/@Model.Image" width="300px" height="150px" />
</div>
<div class="row mt-5">Name: @Model.Name</div>
<div class="row mt-5">FirstContent: @Model.EventDetail.FirstContent</div>
<div class="row mt-5">SecondContent: @Model.EventDetail.SecondContent</div>
<div class="row mt-5">ThirdContent: @Model.EventDetail.ThirdContent</div>
<div class="row mt-5">PlacedArea: @Model.EventDetail.DetailedPlacedArea</div>
<div class="row mt-5">Organized Day: @Model.OrganizedDay</div>
<div class="row mt-5">Start: @Model.StartTime</div>
<div class="row mt-5">End: @Model.EndTime</div>
<div class="row mt-5">
    <a asp-action="Index" class="btn btn-dark">Go back</a>
</div>


