@model Course
@{ ViewData["Title"] = "Detail"; }

<div class="row">
    <img src="~/assets/img/course/@Model.Image" width="300px" height="150px" />
</div>
<div class="row mt-5">Name: @Model.Name</div>
<div class="row mt-5">Description: @Model.Description</div>
<div class="row mt-5">Language: @Model.CourseDetail.Language</div>
<div class="row mt-5">SkillLevel: @Model.CourseDetail.SkillLevel</div>
<div class="row mt-5">StudentsCount: @Model.CourseDetail.StudentsCount</div>
<div class="row mt-5">Duration: @Model.CourseDetail.Duration</div>
<div class="row mt-5">About: @Model.CourseDetail.About</div>
<div class="row mt-5">ApplyInfo: @Model.CourseDetail.AboutApply</div>
<div class="row mt-5">CertificationInfo: @Model.CourseDetail.AboutCertification</div>
<div class="row mt-5">MainContent:  @Model.CourseDetail.Content</div>
<div class="row mt-5">ClassDuration: @Model.CourseDetail.ClassDuration</div>
<div class="row mt-5">
    <a asp-action="Index" class="btn btn-dark">Go back</a>
</div>


